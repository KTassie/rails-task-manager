<div class="card">
<h1>All Tasks</h1>

<ul>
    <% @tasks.each do |task| %>
    <li>
    <%= check_box_tag "task_ids[]", task.id, task.completed %>
    <%= link_to task.title, task_path(task) %>
    </li>
    <% end %>
</ul>

<%= link_to 'New Task', new_task_path, class: 'btn btn-primary' %>

</div>
